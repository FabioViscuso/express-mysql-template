{
    "name": "express-ts-sql-backend",
    "description": "Boilerplate for building backends with express & mysql",
    "version": "1.0.0",
    "author": "Fabio Viscuso",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/FabioViscuso/express-ts-sql-backend"
    },
    "keywords": [
        "node",
        "heroku",
        "express"
    ],
    "scripts": {
        "test": "jest",
        "lint": "eslint src/ --cache --fix",
        "build": "tsc",
        "build@dev": "tsc --watch",
        "start": "node --enable-source-maps dist/index.js",
        "start@dev": "nodemon --watch dist --watch .env",
        "dev": "concurrently 'npm:lint' 'npm:build@dev' 'npm:start@dev'"
    },
    "main": "dist/index.js",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.30.7",
        "@typescript-eslint/parser": "^5.30.7",
        "concurrently": "^7.3.0",
        "eslint": "^8.15.0",
        "jest": "^28.1.3",
        "nodemon": "^2.0.16",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.7",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.6",
        "@types/node": "^18.6.1",
        "@types/supertest": "^2.0.12",
        "cors": "^2.8.5",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "express-async-errors": "^3.1.1"
    },
    "engines": {
        "node": "16.x"
    }
}
